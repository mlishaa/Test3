<Window x:Class="Test3.LogInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test3"
        mc:Ignorable="d"
        Title="LogInWindow" Height="300" Width="400">
    <StackPanel>
        <TextBlock Text="User ID" Style="{StaticResource textBlocks}" Margin="20,20,0,10"/>
        <TextBox x:Name="txtUserIDLogIn" Style="{StaticResource TextBoxes}" Margin="20,0,20,5" />
        <TextBlock Text="User Password" Style="{StaticResource textBlocks}" Margin="20,5,0,10"/>
        <TextBox x:Name="txtUserPSWLogIn" Style="{StaticResource TextBoxes}" Margin="20,0,20,10" />
        <Button x:Name="btnUserLognIn" Content="Log in" Style="{StaticResource myButtons}" Margin="20" Click="btnUserLognIn_Click"/>
    </StackPanel>
</Window>
